/* pages/feedback/feedback.wxss */

page {
  background-color: #f2f2f2;
}

.feedback {
  width: 702rpx;
  height: 500rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-top: 20rpx;
}
.feedback-head {
  padding-top: 30rpx;
  margin-left: 30rpx;
  width: 650rpx;
  height: 400rpx;
  font-size: 26rpx;
  color: #999;
  line-height: 40rpx; 
}

.feedback-second {
  width: 590rpx;
  height: 88rpx;
  background-image: linear-gradient(-90deg, #fd8137 0%, #fdc337 100%);
  margin: 0 auto;
  margin-top: 91rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  letter-spacing: 2rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 50rpx;
  opacity: 0.5;
}
.active{
  opacity: 1;
}
.feedback-bottom{
  color: #999999;
  font-size: 26rpx;
  margin-left: 590rpx;
}
