.order-body-head{
  width: 750rpx;
  margin-top: 20rpx;
  background-color: #fff;
  height: 200rpx;
}
.order-body-head-left{
  width: 702rpx;
  height: 200rpx;
  margin: 0 auto;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  flex-direction: row;
}
.order-body-head-leftimg{
  width: 188rpx;
  height: 140rpx;
  padding-top: 24rpx;
  position: relative;
}
.order-body-head-leftimg-one{
  width: 188rpx;
  height: 140rpx;
}
.order-body-head-leftimg-one image{
  width: 100%;
  height: 100%;
}
.order-body-head-leftimg-two{
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  top: 24rpx;
}
.order-body-head-leftimg-two image{
  width: 100%;
  height: 100%;
}
.order-body-head-right{
  width: 500rpx;
  height: 126rpx;
  margin-top: 32rpx;
  margin-left: 17rpx;
  display: flex;
  flex-direction: column;
}
.order-body-head-right-one{
  height: 50rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
}
.order-body-head-right-one-left{
  color: #333;
  font-size: 28rpx;
  width: 250rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space:nowrap; 
}
.order-body-head-right-one-right{
  width: 120rpx;
  height: 48rpx;
  border: 1px solid #fec09b;
  border-radius: 5px;
  font-size: 24rpx;
  text-align: center;
  line-height: 48rpx;
  color: #fd8137;
  font-family: MicrosoftYaHei;
}
.order-body-head-right-data{
  font-size: 24rpx;
  color: #999;
}
.order-body-head-right-three{
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
  font-family: MicrosoftYaHei;
  margin-top: 12rpx;
}
.order-body-head-right-three-title{
  color: #666;
  font-size: 26rpx;
}
.order-body-head-right-three-num{
