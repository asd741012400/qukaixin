}
.order-body-head .order-body-head-view{
  flex: 1;
  display: flex;
  align-items: center;
}


.order-body-head-name {
  color: #333;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 200rpx;
}

.order-body-head-view image {
  width: 8rpx;
  height: 14rpx;
  margin-left: 16rpx;
  display: none;
}

.order-body-box {
  width: 120rpx;
  height: 48rpx;
  border: 1px solid #fec09b;
  border-radius: 5px;
  font-size: 24rpx;
  text-align: center;
  line-height: 48rpx;
  color: #fd8137;
  font-family: MicrosoftYaHei;
}

.order-body-date {
  font-size: 24rpx;
  color: #999;
}

.order-body-date text {
  margin-left: 10rpx;
}

.order-body-head-bottom {
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
  font-family: MicrosoftYaHei;
  margin-top: 12rpx;
}

.order-body-head-bottom-roll {
  color: #666;
  font-size: 26rpx;
}

.order-body-head-tottom-mark {
  font-size: 24rpx;
  color: #333;
}
.star{
    display: flex;
