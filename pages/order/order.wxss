.hs_lodingbg{
  font-size: 24rpx;
  line-height: 80rpx;
  width: 100%;
  color: #999;
}
.hs_lodingbg .hs_loding{
  display: flex;
  justify-content: center;
  align-items: center;
  
}

.hs_lodingbg .hs_loding .hs_loding_img{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  z-index: 100;
}
.hs_lodingbg .hs_loding .hs_loding_img image{
  width: 100%;
  height: 100%;
  float: left;
}
.hs_lodingbg .hs_loding .hs_loding_img view{
  width: 33rpx;
  height: 33rpx;
  margin-right: 10rpx;
  animation: myfirst 1s linear infinite normal;
}



@keyframes myfirst {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}