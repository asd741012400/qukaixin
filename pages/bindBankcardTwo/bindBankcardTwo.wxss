.bindBankcardTwo-head{
  width: 750rpx;
  height: 152rpx;
  border-top: 1px solid #e5e5e5;
  background-color: #ffffff;
  display: flex;
  flex-direction: column;
}
.bindBankcardTwo-head-one{
  display: flex;
  flex-direction: row;
  margin-left: 24rpx;
  margin-top: 40rpx;
}
.bindBankcardTwo-head-one-left{
  color: #999999;
  font-size: 26rpx;
  width: 78rpx;
}
.bindBankcardTwo-head-one-right{
  color: #666666;
  font-size: 26rpx;
