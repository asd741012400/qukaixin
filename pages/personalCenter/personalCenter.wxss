page{
  background: #f2f2f2;
}
.hs_user{
  display: flex;
  background: #fff;
}
image{
  width: 100%;
  height: 100%;
  float: left;
}
.hs_user .hs_user_img{
  width: 126rpx;
	height: 126rpx;
	border: solid 1px #e5e5e5;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50%;
  margin-left: 24rpx;
  margin-top: 16rpx;
}
.hs_user .hs_user_img view{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.hs_user .hs_user_img view image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.hs_user .hs_user_name{
  margin-left: 30rpx; 
  margin-top: 28rpx;
  flex: 1;
}
.hs_user .hs_user_name .hs_user_names{
  font-size: 30rpx;
  color: #333333;
}  
.hs_user .hs_user_name .hs_user_level{
  margin-top: 16rpx;
  overflow: hidden;
}
.hs_user .hs_user_name .hs_user_level .hs_user_member{
  background-color: #38373c;
	border-radius: 24px; 
  font-size: 26rpx;
  color: #ffffff;
  float: left;
  display: flex;
  align-items: center;
  padding: 8rpx 24rpx;
}
.hs_user .hs_user_name .hs_user_level .hs_user_member .hs_user_member_l{
  width: 26rpx;
	height: 30rpx;
}
.hs_user .hs_user_name .hs_user_level .hs_user_member .hs_user_member_c{
  flex: 1;
  padding-left: 12rpx;
  padding-right: 18rpx;
}
.hs_user .hs_user_name .hs_user_level .hs_user_member .hs_user_member_r{
  width: 12rpx;
	height: 20rpx;
}


.hs_user .hs_user_set .hs_user_num{
  width: 188rpx;
	height: 48rpx;
  line-height: 48rpx;
	border: solid 1px #fd8137;
  border-right: 0;
  border-radius: 30rpx 0 0 30rpx;
  font-size: 22rpx;
  color: #fd8137;
  text-align: center;
  margin-top: 20rpx;
}
.hs_user .hs_user_set .hs_user_num text{
  font-size: 24rpx;
}
.hs_user .hs_user_set .hs_user_set_num{
  width: 100%;
  position: relative;
  height: 50rpx;
  margin-top: 16rpx;
} 
.hs_user .hs_user_set .hs_user_set_num view{
  width: 34rpx;
  height: 32rpx;
  position: absolute;
  right: 24rpx;
}

.hs_user_data{
  display: flex;
  padding-top: 52rpx;
  padding-bottom: 46rpx;
  background: #fff;
}
.hs_user_data .hs_user_datas{
  flex: 1;
  display: flex;
  text-align: center;
  justify-content: center;
}
.hs_user_data .hs_user_datas .hs_user_data_x{
  height: 58rpx;
  border-right:solid 1px #e5e5e5;
  font-size: 26rpx;
  flex: 1;
  color: #999999;
}
.hs_user_data .hs_user_datas .hs_user_data_x .hs_user_data_num{
  position: relative;
  top: -8rpx;
  color: #333333;
}

.hs_signIn{
  padding: 0 24rpx;
  height: 92rpx;
  align-items: center;
  display: flex;
  margin: 20rpx 0;
  background: #fff;
}
.hs_signIn .hs_signIn_l{
  font-size: 28rpx;
  color: #333333; 
}
.hs_signIn .hs_signIn_c{
  font-size: 24rpx;
  color: #999999;
  margin-left: 38rpx;
  flex: 1;
}
.hs_signIn .hs_signIn_r{
  width: 130rpx;
  font-size: 24rpx;
	height: 60rpx;
}
.hs_signIn .hs_signIn_r view{
  width: 130rpx;
	height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  color: #fff;
	border-radius: 10px;
}
.hs_signIn .hs_signIn_r .hs_signIn_r_already{
  background-image: linear-gradient(-90deg, 
		rgba(253, 129, 55, 0.5) 0%, 
		rgba(253, 195, 55, 0.5) 100%);
	opacity: 0.5;
}
.hs_signIn .hs_signIn_r .hs_signIn_r_not{
  background-image: linear-gradient(-90deg, 
		#fd8137 0%, 
		#fdc337 100%);
}


.hs_more{
  background: #fff;
  padding: 0 24rpx 30rpx;
  font-size: 28rpx;
}
.hs_more .hs_more_title{
  line-height: 90rpx;
  height: 90rpx;
  color: #666666;
  border-bottom: 1px solid #e5e5e5;
}

.hs_more .hs_more_body{
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
}
.hs_more_body_text{
  width: 175rpx;
}
.hs_more .hs_more_body .hs_more_body_text{
  /* flex: 1; */
  justify-content: center;
  align-items: center;
  display: flex;
  flex-direction: column;
  color: #333333;
}
.hs_more .hs_more_body .hs_more_body_text view{
  width: 90rpx;
	height: 90rpx;
  margin-bottom: 20rpx;
  margin-top: 30rpx;
}

.hs_rankingList{
  background: #fff;
  margin-top: 20rpx;
  padding: 0 24rpx;
}
.hs_rankingList .hs_rankingList_title{
  font-size: 28rpx;
  color: #666666;
  line-height: 90rpx;
  height: 90rpx;
  border-bottom: 1px solid #e5e5e5;
}
.hs_rankingList .hs_rankingList_body .hs_rankingList_body_list{
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  align-items: center;
  padding: 10rpx 0;
}
.hs_rankingList .hs_rankingList_body .hs_rankingList_body_list:last-of-type{
  border-bottom: 0;
}
.hs_rankingList .hs_rankingList_body .hs_rankingList_body_list .hs_rankingList_body_list_num{
  width: 40rpx;
  font-size: 26rpx;
  color: #333333;
}
.hs_rankingList .hs_rankingList_body .hs_rankingList_body_list .hs_rankingList_body_list_num image{
  width: 24rpx;
	height: 40rpx;
}
.hs_rankingList .hs_rankingList_body .hs_rankingList_body_list .hs_rankingList_body_list_img{
  width: 70rpx;
	height: 70rpx;
  margin: 0 20rpx;
  border-radius: 50%;
  overflow: hidden;
}
.hs_rankingList_body_list_img image{
  width: 100%;
  height: 100%;
}
.hs_rankingList .hs_rankingList_body .hs_rankingList_body_list .hs_rankingList_body_list_text{
  font-size: 28rpx;
  color: #666666;
  flex: 1;
}


.hs_signIna{
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  z-index: 99999999999;
}
.hs_signIna .hs_signIn_body{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  height: 100%;
}
.hs_signIna .hs_signIn_body .hs_sigIn_body_img{
  width: 358rpx;
	height: 278rpx;
}
.hs_signIna .hs_signIn_body text{
  margin-top: 20rpx;
  font-size: 32rpx;
  color: #ffe100;
}
.hs_sigIn_body_img{
  width:400rpx;
height:325rpx;
position: relative
}
.closeImg{
width: 50rpx;
height: 50rpx;
position: absolute;
top: 0;
right: -50rpx;
}

.clickPayVip{
  width: 377rpx;
	height: 91rpx;
  margin-top: 45rpx;
}