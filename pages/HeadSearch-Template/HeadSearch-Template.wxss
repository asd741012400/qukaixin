
image{
  width: 100%;
  height: 100%;
  float: left;
}

.header_search{
  height: 88rpx;
  display: flex;
  align-items: center;
  background: #fff;
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 300;
  border-bottom: 1px solid #ccc;
}
.header_search .header_search_imgage{
  flex: 1;
  padding-left: 24rpx;
  align-items: center;
  display: flex;
}

.header_search .header_search_imgage .header_search_img{
  width: 28rpx;
	height: 28rpx;
  padding-right: 14rpx;
}
.header_search .header_search_imgage .header_search_text{
  font-size: 24rpx;
  color: #999999;
  flex: 1;
}

.header_search .header_search_address{
  width: 102rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.header_search .header_search_address .header_search_img{
  height: 45rpx;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  border-left: 1px solid #e5e5e5;
}
.header_search .header_search_address .header_search_img view{
  width: 34rpx;
	height: 34rpx;
}