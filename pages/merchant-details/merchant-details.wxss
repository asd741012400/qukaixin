.hs_popBg{
  background: rgba(0, 0, 0, 0.5);
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: 900;
}
.hs_pop_content{
  display: flex;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: center;
}
.hs_pop_content .hs_pop{
  width: 600rpx;
	background-color: #ffffff;
	border-radius: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.hs_pop_content .hs_pop .hs_pop_img{
  width: 86rpx;
	height: 86rpx;
  margin-top: 68rpx;
  margin-bottom: 52rpx;
}

.hs_pop_content .hs_pop .hs_pop_img image{
  width: 100%;
  height: 100%;
}
.hs_pop_content .hs_pop .hs_pop_text1{
  font-size: 28rpx;
  color: #333333;
}
.hs_pop_content .hs_pop .hs_pop_text2{
  font-size: 28rpx;
  color: #666666;
}
.hs_pop_content .hs_pop .hs_pop_btn{
  display: flex;
  margin-top: 60rpx;
  width: 100%;
  line-height: 90rpx;
  border-top: 1px solid #e5e5e5;
  font-size: 28rpx;
}

.hs_pop_content .hs_pop .hs_pop_btn text{
  flex: 1;
  text-align: center;
  color: #999999;
}
.hs_pop_content .hs_pop .hs_pop_btn text:last-of-type{
  border-left: 1px solid #e5e5e5;
  color: #fd8137;
}
.hs_pop_content .hs_pop .hs_pop_btn view{
  flex: 1;
  text-align: center;
  color: #fd8137;
}