/* pages/TouchBalance/TouchBalance.wxss */
page{
  background-color: #f2f2f2;
}
.TouchBalance{
  height: 100%;
  width: 750rpx;
  margin-top: 20rpx;
  background-color: #ffffff;
}
.TouchBalance .TouchBalance-center-head:last-of-type{
  border: 0;
}
.TouchBalance-center{
  width: 702rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
}
.TouchBalance-center-head{
  height: 119rpx;
  width: 100%;
  display: flex;
  flex-flow: row wrap;
  border-bottom: 1px solid #e5e5e5;
  justify-content: space-between;
}
.TouchBalance-center-head-left{
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.TouchBalance-center-head-left-name{
  color: #333333;
  font-size: 28rpx;
}
.TouchBalance-center-head-left-date{
  color: #666666;
  font-size: 22rpx;
  margin-top: 8rpx;
}
.TouchBalance-center-head-right{
  color: #222222;
  font-size: 28rpx;
  display: flex;
  align-items: center;
}
.TouchBalance-center-bottom{
  color: #666666;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  margin-top: 41rpx;
}
.aaa {
  position: fixed;
  bottom: 0;
  height: 100rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.aaa image {
  width: 50rpx;
  height: 50rpx;
  animation: myfirst 1s linear infinite normal;
}

@keyframes myfirst {
  from {
    transform: rotate(0deg);
  }

  to {
    transform: rotate(360deg);
  }
}

.message-center-bottom {
  color: #666;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  margin-top: 41rpx;
}